{
  "name": "the-stack-hub",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "setup": "npm install",
    "setup:full": "npm install && npm run db:setup",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:setup": "npm run db:push && npm run db:seed",
    "db:reset": "npm run db:push && npm run db:seed",
    "db:seed": "npx tsx src/db/seeds/seed-all.ts",
    "db:seed:users": "npx tsx src/db/seeds/users.ts",
    "db:seed:subjects": "npx tsx src/db/seeds/subjects.ts",
    "db:seed:class-levels": "npx tsx src/db/seeds/class-levels.ts",
    "db:seed:chapters": "npx tsx src/db/seeds/chapters.ts",
    "db:seed:questions-it": "npx tsx src/db/seeds/questions-it.ts",
    "db:seed:questions-scholarship": "npx tsx src/db/seeds/questions-scholarship.ts",
    "db:seed:exam-structures": "npx tsx src/db/seeds/exam-structures.ts",
    "db:seed:scheduled-exams": "npx tsx src/db/seeds/scheduled-exams.ts",
    "db:seed:schools": "npx tsx src/db/seeds/schools.ts",
    "db:turso:generate": "drizzle-kit generate --config=drizzle.config.turso.ts",
    "db:turso:push": "drizzle-kit push --config=drizzle.config.turso.ts",
    "db:turso:studio": "drizzle-kit studio --config=drizzle.config.turso.ts",
    "db:turso:migrate": "npx tsx scripts/migrate-to-turso.ts",
    "db:switch:turso": "bash scripts/switch-db.sh turso",
    "db:switch:pg": "bash scripts/switch-db.sh pg",
    "db:switch:status": "bash scripts/switch-db.sh status"
  },
